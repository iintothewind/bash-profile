<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <pluginGroups>
    <pluginGroup>org.mortbay.jetty</pluginGroup>
    <pluginGroup>com.ebay.raptor.build</pluginGroup>
    <pluginGroup>org.codehaus.mojo</pluginGroup>
    <pluginGroup>org.sonatype.maven.plugins</pluginGroup>
  </pluginGroups>
  <proxies>
    <!--
    <proxy>
    <id>BlueSSO</id>
    <active>true</active>
    <protocol>http</protocol>
    <host>3.20.128.6</host>
    <port>88</port>
    <nonProxyHosts>127.0.0.1,localhost,*.ge.com</nonProxyHosts>
    </proxy>
  -->
  </proxies>
  <servers>
    <servers>
      <server>
        <id>raptor.releases</id>
        <username>RaptorTeam</username>
        <password>RaptorTeam</password>
      </server>
      <server>
        <id>raptor.snapshots</id>
        <username>RaptorTeam</username>
        <password>RaptorTeam</password>
      </server>
    </servers>
  </servers>
  <mirrors>
    <!--
      <mirror>
      <id>aliyun</id>
      <mirrorOf>*</mirrorOf>
      <name>aliyun-release</name>
      <url>http://maven.aliyun.com/nexus/content/groups/public</url>
      </mirror>
    -->
    <mirror>
      <id>central</id>
      <mirrorOf>*,!public</mirrorOf>
      <url>https://paypalcentral.es.paypalcorp.com/nexus/content/repositories/central</url>
    </mirror>
  </mirrors>

  <profiles>
    <profile>
      <id>default</id>
      <properties>
        <downloadSources>true</downloadSources>
        <!--<downloadJavadocs>true</downloadJavadocs>-->
      </properties>
      <repositories>
        <repository>
          <id>aliyun-release</id>
          <url>http://maven.aliyun.com/nexus/content/groups/public</url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <releases>
            <enabled>true</enabled>
          </releases>
        </repository>
        <repository>
          <id>bintray-release</id>
          <url>http://oss.jfrog.org/artifactory/libs-release</url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
        <repository>
          <id>oss</id>
          <url>https://oss.sonatype.org/content/groups/public</url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
      </repositories>

      <pluginRepositories>
        <pluginRepository>
          <id>aliyun-plugins</id>
          <url>http://maven.aliyun.com/nexus/content/groups/public</url>
        </pluginRepository>
        <pluginRepository>
          <id>spring-plugins</id>
          <url>https://repo.spring.io/libs-release</url>
        </pluginRepository>
      </pluginRepositories>
    </profile>
    <profile>
      <id>central</id>
      <repositories>
        <repository>
          <id>central</id>
          <url>http://repo1.maven.org/maven2</url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <releases>
            <enabled>true</enabled>
          </releases>
        </repository>
      </repositories>

      <pluginRepositories>
        <pluginRepository>
          <id>central</id>
          <url>http://repo1.maven.org/maven2</url>
        </pluginRepository>
      </pluginRepositories>
    </profile>
    <profile>
      <id>paypal</id>
      <!-- add a repository property to ease the archetype:generate useage -->
      <properties>
        <archetypeCatalog>https://paypalcentral.es.paypalcorp.com/nexus/content/groups/public/archetype-catalog.xml</archetypeCatalog>
      </properties>
      <repositories>
        <!-- Raptor Repository -->
        <repository>
          <id>public</id>
          <url>https://paypalcentral.es.paypalcorp.com/nexus/content/groups/public/</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>public</id>
          <url>https://paypalcentral.es.paypalcorp.com/nexus/content/groups/public/</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </pluginRepository>
        <pluginRepository>
          <id>central</id>
          <url>https://paypalcentral.es.paypalcorp.com/nexus/content/repositories/central</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>

  <activeProfiles>
    <activeProfile>paypal</activeProfile>
  </activeProfiles>
</settings>
