<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.2.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.2.0 http://maven.apache.org/xsd/settings-1.2.0.xsd">
  <pluginGroups>
    <pluginGroup>org.mortbay.jetty</pluginGroup>
    <pluginGroup>org.sonatype.maven.plugins</pluginGroup>
  </pluginGroups>
    <servers>
      <server>
        <id>raptor.releases</id>
        <username>RaptorTeam</username>
        <password>RaptorTeam</password>
      </server>
      <server>
        <id>raptor.snapshots</id>
        <username>RaptorTeam</username>
        <password>RaptorTeam</password>
      </server>
      <server>
        <id>releases</id>
        <username>wise</username>
        <password>admin</password>
      </server>
      <server>
        <id>snapshots</id>
        <username>wise</username>
        <password>admin</password>
      </server>
    </servers>
  <mirrors>
    <mirror>
        <id>maven-default-http-blocker</id>
        <mirrorOf>external:http:*</mirrorOf>
        <name>pseudo</name>
        <url>http://0.0.0.0/</url>
        <blocked>false</blocked>
    </mirror>
    <mirror>
      <id>aliyun</id>
      <mirrorOf>external:http:*</mirrorOf>
      <name>aliyun-release</name>
      <url>http://maven.aliyun.com/nexus/content/groups/public</url>
    </mirror>
    <mirror>
      <id>wise</id>
      <mirrorOf>external:http:*</mirrorOf>
      <url>http://42.192.12.138:8681/nexus/repository/maven-public</url>
    </mirror>
  </mirrors>

  <profiles>
    <profile>
      <id>default</id>
      <properties>
        <downloadSources>true</downloadSources>
        <!--<downloadJavadocs>true</downloadJavadocs>-->
      </properties>
      <repositories>
        <repository>
          <id>aliyun-release</id>
          <url>https://maven.aliyun.com/nexus/content/groups/public</url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <releases>
            <enabled>true</enabled>
          </releases>
        </repository>
        <repository>
          <id>bintray-release</id>
          <url>https://oss.jfrog.org/artifactory/libs-release</url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
        <repository>
          <id>oss</id>
          <url>https://oss.sonatype.org/content/groups/public</url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
      </repositories>

      <pluginRepositories>
        <pluginRepository>
          <id>aliyun-plugins</id>
          <url>https://maven.aliyun.com/nexus/content/groups/public</url>
        </pluginRepository>
        <pluginRepository>
          <id>spring-plugins</id>
          <url>https://repo.spring.io/libs-release</url>
        </pluginRepository>
      </pluginRepositories>
    </profile>
    <profile>
      <id>central</id>
      <repositories>
        <repository>
          <id>central</id>
          <url>https://repo1.maven.org/maven2</url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <releases>
            <enabled>true</enabled>
          </releases>
        </repository>
      </repositories>

      <pluginRepositories>
        <pluginRepository>
          <id>central</id>
          <url>https://repo1.maven.org/maven2</url>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>

  <activeProfiles>
    <activeProfile>default</activeProfile>
  </activeProfiles>
</settings>
